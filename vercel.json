{
  "routes": [
    { "src": "/llms.txt", "dest": "/llms.txt" },

    { "src": "/robots.txt", "dest": "/robots.txt" },
    { "src": "/sitemap.xml", "dest": "/sitemap.xml" },

    { "src": "/terms/?", "dest": "/terms.html" },
    { "src": "/privacy/?", "dest": "/privacy.html" },
    { "src": "/contact/?", "dest": "/contact.html" },
    { "src": "/blog/community-marketing-2026/?", "dest": "/blog/community-marketing-2026.html" },

    { "src": "/(.*)\\.(css|js|map|png|jpg|jpeg|gif|svg|webp|ico|txt|xml|json|woff|woff2|ttf|eot)$", "dest": "/$1" },

    { "src": "/(assets|static|images|img|fonts)/(.*)", "dest": "/$1/$2" },

    { "src": "/(.*)", "dest": "/404.html", "status": 404 }
  ]
}
